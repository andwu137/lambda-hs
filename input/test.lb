s = \f -> \g -> \x -> f (g x) x;
k = \x -> \y -> x;
i = \x -> x;
y = \f -> (\x -> f (x x)) (\x -> f (x x));
joinArrow = \f x -> f x x;

fac = \x ->
    if (x==0)
        1
        (x * fac (x-1));

fib = fibHelper 0 1;
fibHelper = \x -> \y -> \n -> if (n==0) x ~$ fibHelper y (x+y) (n-1);

fib' x =
    if (x==0) 0 ~$
    if (x==1) 1 ~$
    fib' (x-1) + fib' (x-2);

fib'' f g x =
    if (x==0) 0 ~$
    if (x==1) 1 ~$
    f (x-1) + g (x-2);

fib''' = joinArrow fib'';

fib'''' = y fib''';

(~$) f x = f x;
(.) f g x = f (g x);
